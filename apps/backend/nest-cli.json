{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": false,
    "builder": "tsc",
    "assets": [
      {
        "include": "database/migrations/*.sql",
        "outDir": "dist"
      }
    ]
  }
}
